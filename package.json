{
  "name": "user-data-api",
  "version": "1.0.0",
  "description": "High-performance Express.js API with advanced caching, rate limiting, and asynchronous processing",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/api-gateway/index.js",
    "start:user-service": "node dist/services/user-service/index.js",
    "start:cache-service": "node dist/services/cache-service/index.js",
    "start:gateway": "node dist/api-gateway/index.js",
    "dev": "ts-node-dev --respawn --transpile-only api-gateway/index.ts",
    "dev:user-service": "ts-node-dev --respawn --transpile-only services/user-service/index.ts",
    "dev:cache-service": "ts-node-dev --respawn --transpile-only services/cache-service/index.ts",
    "dev:gateway": "ts-node-dev --respawn --transpile-only api-gateway/index.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "mocha --require ts-node/register --timeout 10000 'services/**/*.test.ts' 'api-gateway/**/*.test.ts' --exit",
    "test:watch": "mocha --require ts-node/register --watch 'services/**/*.test.ts' 'api-gateway/**/*.test.ts' --exit"
  },
  "keywords": [
    "express",
    "typescript",
    "caching",
    "rate-limiting",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "joi": "^18.0.2",
    "mongoose": "^8.0.0",
    "redis": "^4.6.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.0",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.3",
    "chai": "^4.3.0",
    "mocha": "^10.2.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
